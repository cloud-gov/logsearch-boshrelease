{
  "template": {
    "settings": {
      "index": {
        "query": {
          "default_field": "@raw"
        }
      }
    },
    "mappings": {
      "dynamic_templates": [
        {
          "string_fields": {
            "match": "*",
            "match_mapping_type": "string",
            "mapping": {
              "type": "keyword",
              "index": true,
              "norms": false
            }
          }
        },
        {
          "app-nano-seconds": {
          "match": "app.sseLogTime",
          "match_mapping_type" : "date",
          "mapping"{
            "type": "date".
            "format": "strict_date_optional_time_nanos"
          }
        }
      ],
      "properties": {
        "@version": {
          "type": "keyword",
          "index": true
        },
        "@raw": {
          "type": "text",
          "index": true,
          "norms": false
        },
        "geoip": {
          "type": "object",
          "dynamic": true,
          "properties": {
            "location": {
              "type": "geo_point"
            }
          }
        }
      }
    }
  }
}
